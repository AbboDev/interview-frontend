        "json-server": "^0.17.1",
        "json-server-auth": "^2.1.0",
        "postcss": "^8.4.19",
        "prettier": "^2.7.1",
        "sass": "^1.56.1",
        "stylelint": "^14.15.0",
        "stylelint-config-prettier": "^9.0.4",
        "stylelint-config-standard-scss": "^6.1.0",
        "typescript": "^4.8.4",
        "vite": "^3.2.3",
        "vite-plugin-eslint": "^1.8.1",
